{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc", "closure"]
  },
  "source": {
    "includePattern": ".+\\.js(doc|x)?$",
    "include": ["src"],
    "exclude": ["node_modules"]
  },
  "recurseDepth": 10,
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "README.md"
  }
}
